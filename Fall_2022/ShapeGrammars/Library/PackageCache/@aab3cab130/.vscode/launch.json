    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Dynamic Query Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "DynamicQueryBuild",
            "program": "${workspaceFolder}/Examples/DynamicQuery/bin/Debug/net5.0/DynamicQuery.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Examples/DynamicQuery",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}